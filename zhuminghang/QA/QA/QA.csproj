<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net48</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
	</PropertyGroup>



	<ItemGroup>
		<PackageReference Include="DevExpress.RichEdit.Core" Version="22.1.4" />
		<PackageReference Include="DevExpress.Win" Version="22.1.4" />
		<PackageReference Include="DevExpress.Win.BonusSkins" Version="22.1.4" />
		<PackageReference Include="DevExpress.Win.RichEdit" Version="22.1.4" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="NPOI" Version="2.5.4" />
	</ItemGroup>



	<ItemGroup>
		<ProjectReference Include="..\WinControls\WinControls.csproj" />
	</ItemGroup>



	<ItemGroup>
		<Reference Include="DevExpress.Office.v22.1.Core">
			
		</Reference>
		<Reference Include="DevExpress.RichEdit.v22.1.Core">
			
		</Reference>
		<Reference Include="DevExpress.XtraRichEdit.v22.1">
			
		</Reference>
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Web" />
	</ItemGroup>



	<ItemGroup>
	  <Compile Update="Properties\Resources.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>



	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>



	<ItemGroup>
		<None Update="Resources\default.png">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Update="Resources\image.jpg">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>



	<ItemGroup>
	  <Folder Include="json\" />
	</ItemGroup>

</Project>